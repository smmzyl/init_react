{
  "name": "init-react",
  "version": "1.0.0",
  "description": "init react demo app",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc --noEmit",
    "prepare": "husky install",
    "commit": "git-cz",
    "release": "standard-version",
    "start:dev": "vite --mode development",
    "start:test": "vite --mode testing",
    "start:staging": "vite --mode staging",
    "start:prod": "vite --mode production",
    "build": "pnpm build:prod",
    "build:dev": "pnpm tsc && vite build --mode development",
    "build:test": "pnpm tsc && vite build --mode testing",
    "build:staging": "pnpm tsc && vite build --mode staging",
    "build:prod": "pnpm tsc && vite build --mode production"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "parser": "babel-eslint"
  },
  "keywords": [
    "react",
    "typescript",
    "vite"
  ],
  "dependencies": {
    "@vitejs/plugin-react": "^4.0.3",
    "babel-eslint": "^10.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "pnpm": "^8.6.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "standard-version": "^9.5.0",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "cz-git": "^1.7.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "prettier-eslint": "^15.0.1",
    "sass": "^1.64.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vite-plugin-eslint": "^1.8.1"
  },
  "author": "sakura_kimi",
  "license": "ISC"
}
